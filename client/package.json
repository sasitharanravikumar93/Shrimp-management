{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "@mui/x-date-pickers": "^7.29.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "date-fns": "^2.28.0",
    "framer-motion": "^6.3.3",
    "i18next": "^25.3.6",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "localforage": "^1.10.0",
    "moment": "^2.29.4",
    "papaparse": "^5.3.2",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-big-calendar": "^1.5.1",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.31.2",
    "react-i18next": "^15.6.1",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "recharts": "^2.1.10",
    "stylis": "^4.3.6",
    "stylis-plugin-rtl": "^2.1.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "build:analyze": "ANALYZE_BUNDLE=true craco build",
    "build:prod": "NODE_ENV=production craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "analyze:interactive": "npx webpack-bundle-analyzer build/static/js/*.js",
    "preload-routes": "node scripts/preload-routes.js",
    "optimize-images": "node scripts/optimize-images.js",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:quiet": "eslint src --ext .js,.jsx,.ts,.tsx --quiet",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,md}'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,css,md}'",
    "type-check": "tsc --noEmit",
    "quality": "npm run lint && npm run format:check && npm run type-check",
    "prepare": "cd .. && husky install client/.husky",
    "env:docs": "node scripts/generate-env-docs.js",
    "env:validate": "node -e \"require('./src/config/environment.js')\""
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "babel-plugin-import": "^1.13.8",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "glob": "^11.0.3",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "source-map-explorer": "^2.5.3",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "^5.0.0",
    "webpack-bundle-analyzer": "^4.9.0"
  }
}
